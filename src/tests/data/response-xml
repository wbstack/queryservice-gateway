This is just testing the replacements in a format other than JSON
--------
www.wikidata.org
--------
PREFIX wd: <http://www.wikidata.org/entity/>
QUERY
--------
PREFIX wd: <http://wdinternal/entity/>
QUERY
--------
<?xml version='1.0' encoding='UTF-8'?>
<sparql xmlns='http://www.w3.org/2005/sparql-results#'>
	<head>
		<variable name='item'/>
		<variable name='itemLabel'/>
	</head>
	<results>
		<result>
			<binding name='item'>
				<uri>http://wdinternal/entity/Q151345</uri>
			</binding>
			<binding name='itemLabel'>
				<literal xml:lang='en'>Billygoat Hennes</literal>
			</binding>
		</result>
		<result>
			<binding name='item'>
				<uri>http://wdinternal/entity/Q3569037</uri>
			</binding>
			<binding name='itemLabel'>
				<literal xml:lang='en'>William Windsor</literal>
			</binding>
		</result>
	</results>
</sparql>
--------
<?xml version='1.0' encoding='UTF-8'?>
<sparql xmlns='http://www.w3.org/2005/sparql-results#'>
	<head>
		<variable name='item'/>
		<variable name='itemLabel'/>
	</head>
	<results>
		<result>
			<binding name='item'>
				<uri>http://www.wikidata.org/entity/Q151345</uri>
			</binding>
			<binding name='itemLabel'>
				<literal xml:lang='en'>Billygoat Hennes</literal>
			</binding>
		</result>
		<result>
			<binding name='item'>
				<uri>http://www.wikidata.org/entity/Q3569037</uri>
			</binding>
			<binding name='itemLabel'>
				<literal xml:lang='en'>William Windsor</literal>
			</binding>
		</result>
	</results>
</sparql>
